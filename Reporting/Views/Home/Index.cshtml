@model Reporting.Models.IndexModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />

    <link href="~/Scripts/jqPlot/jquery.jqplot.min.css" rel="stylesheet" />

    <link href="~/Content/Reporting.css" rel="stylesheet" />
</head>
<body>

    <div class="main-container" id="main-container">

        <div class="row">

            <div class="col-xs-12">

                @foreach (var widget in Model.Widgets)
                {
                <div class="col-sm-4">

                    @Html.Partial("_Widget", widget)

                </div>
                }

            </div>

        </div>

    </div>

</body>


</html>
<script src="~/Scripts/jquery-2.0.3.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/knockout-3.0.0.js"></script>
<script src="~/Scripts/flot/jquery.flot.min.js"></script>
<script src="~/Scripts/flot/jquery.flot.pie.min.js"></script>
<script src="~/Scripts/jquery.jqGrid.min.js"></script>
<script src="~/Scripts/jquery.easy-pie-chart.min.js"></script>

<script src="~/Scripts/Widget.js" type="text/javascript"></script>